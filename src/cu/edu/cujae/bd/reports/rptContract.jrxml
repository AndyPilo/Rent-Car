	<queryString>
		<![CDATA[SELECT tourist.name,car.plate,brand.name_brand,model.name_model,payment.payment,
		   date_contract.start_date,date_contract.final_date,contract.extension,driver.name
	FROM contract INNER JOIN tourist ON contract.cod_tourist = tourist.cod_tourist
				  INNER JOIN car ON contract.cod_car = car.cod_car
				  INNER JOIN model ON car.cod_model = model.cod_model
				  INNER JOIN brand ON  model.cod_brand = brand.cod_brand
				  INNER JOIN payment ON contract.cod_payment = payment.cod_payment
				  INNER JOIN driver ON contract.cod_driver = driver.cod_driver
				  INNER JOIN date_contract ON contract.cod_date = date_contract.id_date;]]>
	</queryString>
